{
    "name": "veneridze/laravel-autologin",
    "description": "Autologin package for Laravel 11",
    "keywords": [
        "laravel",
        "autologin"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Veneridze",
            "email": "dwightVeneridze@studious.com.au"
        },
        {
            "name": "Veneridze",
            "email": "viacheslav.kopaev@yandex.ru"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "11.* | 12.*",
        "spatie/laravel-package-tools": "^1.18"
    },
    "autoload": {
        "classmap": [
            "src/controllers"
        ],
        "psr-4": {
            "Veneridze\\Autologin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Veneridze\\Autologin\\AutologinProvider"
            ],
            "aliases": {
                "Autologin": "Veneridze\\Autologin\\Facades\\Autologin"
            }
        }
    },
    "minimum-stability": "stable"
}